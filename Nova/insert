insert (K key, E elt) into (Map KE map)
  if map
    if
     key < #k map.key -> map.left  := insert (key, elt) into map.left
     key > #k         -> map.right := insert (key, elt) into map.right
      else
      map.elt := elt
    else
    map (void, key, elt, void)

update (K key, E elt) of (Map KE map) with (E <- E f)

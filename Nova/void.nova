synonym Void = ()

Void void = void

Bool something (A a)
  tcase a
    Void -> false
    _ -> true

Bool null (A a)
  tcase a
    Void -> true
    _ -> false

Void void (A _) = void
Void void (A _,B _) = void
Void void (A _,B _,C _) = void
Void void (A _,B _,C _,D _) = void
Void void (A _,B _,C _,D _,E _) = void
Void void (A _,B _,C _,D _,E _,F _) = void
Void void (A _,B _,C _,D _,E _,F _,G _) = void

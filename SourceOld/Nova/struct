Cons E
    E      car
    List E cdr

Delay A
    A <- () fn
    Bool    done
    A       result

Matrix E
    Array E array
    [Int]   dimensions

Runtime
    List SpecBind specials
    ...
 
Sequence E
    Array E array
    Int     start
    Int     end

Stack E
    Array E array
    Int     size
    Int     fill

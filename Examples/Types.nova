type Bool = false | true

type Even Int a = a % 2 == 0

type Odd Int a = not (Even a)

type Table ELT = (Array ELT, Int)

type Sequence ELT = (Array ELT, Int, Int)

type Cons ELT = (ELT, List ELT)

type List ELT = Cons ELT | Void

type Void = ()
void = ()

type Map KEY VALUE = (Map KEY VALUE left, KEY key, VALUE value, Map KEY VALUE right) | Void
Map K V empty = void

type Set VALUE = (Set VALUE left, VALUE value, Set VALUE right) | Void
Set V empty = void

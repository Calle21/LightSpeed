Access     = unsigned
Char       = unsigned 8
Fail       = fail
Float      = floating
Int        = signed

Map KE = map (Map KE left
              K      key
              E      elt
              Map KE right) | Void

Maps E = maps (Maps E left
               String key
               E      elt
               Maps E right) | Void \\ ?

Set E = set (Set E left
             E     elt
             Set E right) | Void


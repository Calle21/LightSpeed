find (R thing) in ({E} seq) with (R <- E fn)
  end = seq.end
  find (i = seq.start)
    if
     i            == end   -> fail
     fn #r seq[i] == thing -> #r
      else
      find (i + 1)
find (E elt) in (Map KE map)
  if map
    if map.elt == elt
      map
      else
      find elt in #(map.left `failor` map.right)
    else
    fail

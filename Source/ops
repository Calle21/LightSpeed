postfix ! ?          \\ factorial, is

prefix ~ !!          \\ converse, not

infixl 9 `prefix`    \\ prefix

infixr 9 << && ||    \\ compose, and, or

infixr 8 ^           \\ exp

infixl 7 * /         \\ times, over

infixl 6 + -         \\ plus, minus

infixr 5 : ;         \\ cons, dcons

infixl 4 => == ~~ /= \\ isin, equal, almostequal, notequal
